       /*************************************
        ****  FUNCTION: calcular_distancia_formacion
        *************************************/
MakeFunction( calcular_distancia_formacion, [],
SetValue(Tren_Actual:Distancia_A_Proxima_Formacion, Recorrido_Actual:Posicion_Siguiente_Tren - Tren_Actual:Posicion) );

       /*************************************
        ****  FUNCTION: calcular_distancia_estacion
        *************************************/
MakeFunction( calcular_distancia_estacion, [],
SetValue(Tren_Actual:Distancia_A_Proxima_Estacion, Proxima_Estacion:Posicion - Tren_Actual:Posicion) );

       /*************************************
        ****  FUNCTION: calcular_distancia_curva
        *************************************/
MakeFunction( calcular_distancia_curva, [],
SetValue(Tren_Actual:Distancia_A_Proxima_Curva, Proxima_Curva:Posicion_Inicio - Tren_Actual:Posicion) );

       /*************************************
        ****  FUNCTION: calcular_velocidad_curva
        *************************************/
MakeFunction( calcular_velocidad_curva, [],
SetValue(Proxima_Curva:Velocidad, 2.2 * Sqrt(Proxima_Curva:Radio) * Condicion_Climatica_Actual:Factor * Capacidad_Tren_Actual:Factor) );

       /*************************************
        ****  FUNCTION: factor_condicion_climatica
        *************************************/
MakeFunction( factor_condicion_climatica, [],
{
If Condicion_Climatica_Actual:Descripcion #= "Despejado"
Then
   SetValue(Condicion_Climatica_Actual:Factor, 1); 

If Condicion_Climatica_Actual:Descripcion #= "Nieve"
Then
   SetValue(Condicion_Climatica_Actual:Factor, 0.7); 

If Condicion_Climatica_Actual:Descripcion #= "Lluvia"
Then
   SetValue(Condicion_Climatica_Actual:Factor, 0.85); 

If Condicion_Climatica_Actual:Descripcion #= "Niebla"
Then
   SetValue(Condicion_Climatica_Actual:Factor, 0.85);

} );

       /*************************************
        ****  FUNCTION: factor_capacidad
        *************************************/
MakeFunction( factor_capacidad, [],
{
If Capacidad_Tren_Actual:Valor >= 75
Then
   SetValue(Capacidad_Tren_Actual:Factor, 1); 

If Capacidad_Tren_Actual:Valor < 75 And Capacidad_Tren_Actual:Valor >= 50
Then
   SetValue(Capacidad_Tren_Actual:Factor, 0.95); 

If Capacidad_Tren_Actual:Valor < 50 And Capacidad_Tren_Actual:Valor >= 25
Then
   SetValue(Capacidad_Tren_Actual:Factor, 0.9); 

If Capacidad_Tren_Actual:Valor < 25
Then
   SetValue(Capacidad_Tren_Actual:Factor, 0.85);

} );

       /*************************************
        ****  FUNCTION: calcular_velocidad_maxima
        *************************************/
MakeFunction( calcular_velocidad_maxima, [],
SetValue(Recorrido_Actual:Velocidad_Maxima_Permitida, Recorrido_Actual:Velocidad_Maxima_Reglamentaria * Condicion_Climatica_Actual:Factor * Capacidad_Tren_Actual:Factor) );

       /*************************************
        ****  FUNCTION: transitando_curva
        *************************************/
MakeFunction( transitando_curva, [],
If Tren_Actual:Posicion < Recorrido_Actual:Proxima_Curva:Posicion_Fin 
    And Tren_Actual:Posicion > Recorrido_Actual:Proxima_Curva:Posicion_Inicio
Then
    SetValue(Tren_Actual:Transitando_Curva, true)
Else
    SetValue(Tren_Actual:Transitando_Curva, false) );

       /*************************************
        ****  FUNCTION: distancia_min_frenado_estacion
        *************************************/
MakeFunction( distancia_min_frenado_estacion, [],
SetValue(Proxima_Estacion:Distancia_Minima_Frenado, 
    Tren_Actual:Velocidad_Actual * 10 / (Condicion_Climatica_Actual:Factor * Capacidad_Tren_Actual:Factor)) );

       /*************************************
        ****  FUNCTION: distancia_min_frenado_curva
        *************************************/
MakeFunction( distancia_min_frenado_curva, [],
SetValue(Proxima_Curva:Distancia_Minima_Frenado, 
    (Tren_Actual:Velocidad_Actual - Proxima_Curva:Velocidad) * 10 / (Condicion_Climatica_Actual:Factor * Capacidad_Tren_Actual:Factor)) );

       /*************************************
        ****  FUNCTION: calcular_accion_sugerida
        *************************************/
MakeFunction( calcular_accion_sugerida, [],
{
inicializar();
ForwardChain([NOASSERT],NULL,Global:Reglas);
PostMessage(Tren_Actual:Accion_Sugerida);
} );

       /*************************************
        ****  FUNCTION: inicializar
        *************************************/
MakeFunction( inicializar, [],
{
factor_capacidad();
factor_condicion_climatica();
calcular_distancia_formacion();
calcular_distancia_estacion();
calcular_distancia_curva();
calcular_velocidad_curva();
calcular_velocidad_maxima();
transitando_curva();
distancia_min_frenado_estacion();
distancia_min_frenado_curva();
} );



