/*********************************************************/
/**         ALL RULES ARE SAVED BELOW             **/
/*********************************************************/

       /*************************************
        ****  RULE: R1.DisminuirAVMP
        *************************************/
MakeRule( R1.DisminuirAVMP, [Tren_Actual|Tren Recorrido_Actual|Recorrido],
    Tren_Actual:Velocidad_Actual > Recorrido_Actual:Velocidad_Maxima_Permitida,
    SetValue( Tren_Actual:Accion_Sugerida, "Disminuya velocidad a "
                                               # Recorrido_Actual:Velocidad_Maxima_Permitida ) );

       /*************************************
        ****  RULE: R2.DetenerFormacion
        *************************************/
MakeRule( R2.DetenerFormacion, [Proximo_Semaforo|Semaforo],
    Proximo_Semaforo:Color #= Rojo,
    SetValue( Tren_Actual:Accion_Sugerida, "Detención inmediata de la formación" ) );

       /*************************************
        ****  RULE: R3.MantenerVelocidadActual
        *************************************/
MakeRule( R3.MantenerVelocidadActual, [Tren_Actual|Tren],
    {
    Tren_Actual:Velocidad_Actual #= 0 And Tren_Actual:Estado_Puertas
        #= Abiertas And Tren_Actual:Distancia_A_Proxima_Estacion
        #= 0;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Mantenga velocidad" ) );

       /*************************************
        ****  RULE: R4.DisminuirAVelDeCurva
        *************************************/
MakeRule( R4.DisminuirAVelDeCurva, [Tren_Actual|Tren Proxima_Curva|Curva],
    {
    Tren_Actual:Velocidad_Actual > Proxima_Curva:Velocidad And
        Tren_Actual:Distancia_A_Proxima_Curva > Proxima_Curva:Distancia_Minima_Frenado;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Disminuya velocidad a "
                                               # Proxima_Curva:Velocidad ) );

       /*************************************
        ****  RULE: R5.DetenerseEnEstacion
        *************************************/
MakeRule( R5.DetenerseEnEstacion, [Tren_Actual|Tren Proxima_Estacion|Estacion],
    {
    Tren_Actual:Velocidad_Actual > 0 And Tren_Actual:Distancia_A_Proxima_Estacion
        <= Proxima_Estacion:Distancia_Minima_Frenado;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Disminuya velocidad a 0" ) );

       /*************************************
        ****  RULE: R6.DisminurVelPorFormacion
        *************************************/
MakeRule( R6.DisminurVelPorFormacion, [Tren_Actual|Tren],
    {
    Tren_Actual:Velocidad_Actual > 12 And Tren_Actual:Distancia_A_Proxima_Formacion
        < 1000;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Disminuya velocidad a 12" ) );

       /*************************************
        ****  RULE: R7.DetenerFormacionPorFormacion
        *************************************/
MakeRule( R7.DetenerFormacionPorFormacion, [Tren_Actual|Tren],
    Tren_Actual:Distancia_A_Proxima_Formacion < 500,
    SetValue( Tren_Actual:Accion_Sugerida, "Detención inmediata de la formación" ) );

       /*************************************
        ****  RULE: R8.DisminuirVelSemaforoAmarillo
        *************************************/
MakeRule( R8.DisminuirVelSemaforoAmarillo, [Tren_Actual|Tren Proximo_Semaforo|Semaforo],
    {
    Tren_Actual:Velocidad_Actual > 12 And Proximo_Semaforo:Color
        #= Amarillo;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Disminuya velocidad a 12" ) );

       /*************************************
        ****  RULE: R9.MantenerVelSemaforoAmarillo
        *************************************/
MakeRule( R9.MantenerVelSemaforoAmarillo, [Tren_Actual|Tren Proximo_Semaforo|Semaforo],
    {
    Tren_Actual:Velocidad_Actual #= 12 And Proximo_Semaforo:Color
        #= Amarillo;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Mantenga velocidad" ) );

       /*************************************
        ****  RULE: R10.AumentarVelocidadAVMP
        *************************************/
MakeRule( R10.AumentarVelocidadAVMP, [Tren_Actual|Tren Recorrido_Actual|Recorrido Proximo_Semaforo|Semaforo Proxima_Curva|Curva],
    {
    Tren_Actual:Velocidad_Actual < Recorrido_Actual:Velocidad_Maxima_Permitida
        And Tren_Actual:Distancia_A_Proxima_Formacion >= 1000
        And Proximo_Semaforo:Color #= Verde And Tren_Actual:Distancia_A_Proxima_Curva
        >= Proxima_Curva:Distancia_Minima_Frenado And Tren_Actual:Transitando_Curva
        #= FALSE;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Aumente velocidad a "
                                               # Recorrido_Actual:Maxima_Velocidad_Permitida ) );

       /*************************************
        ****  RULE: R11.AumentarVelSalidaEstacion
        *************************************/
MakeRule( R11.AumentarVelSalidaEstacion, [Tren_Actual|Tren Proximo_Semaforo|Semaforo],
    {
    Tren_Actual:Velocidad_Actual #= 0 And Tren_Actual:Estado_Puertas
        #= Cerradas And Proximo_Semaforo:Color #= Verde And Tren_Actual:Distancia_A_Proxima_Estacion
        #= 0 And Tren_Actual:Transitando_Curva #= FALSE;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Aumente velocidad a "
                                               # Recorrido_Actual:Maxima_Velocidad_Permitida ) );

       /*************************************
        ****  RULE: R12.AumVeldSalidaEstacionAma
        *************************************/
MakeRule( R12.AumVeldSalidaEstacionAma, [Tren_Actual|Tren Proximo_Semaforo|Semaforo],
    {
    Tren_Actual:Velocidad_Actual #= 0 And Tren_Actual:Estado_Puertas
        #= Cerradas And Proximo_Semaforo:Color #= Amarillo And
        Tren_Actual:Distancia_A_Proxima_Estacion #= 0 And Tren_Actual:Transitando_Curva
        #= FALSE;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Aumente velocidad a 12" ) );

       /*************************************
        ****  RULE: R13.AumentarVelocidadAVMinP
        *************************************/
MakeRule( R13.AumentarVelocidadAVMinP, [Tren_Actual|Tren Recorrido_Actual|Recorrido Proximo_Semaforo|Semaforo Proxima_Estacion|Estacion Proxima_Curva|Curva],
    {
    Tren_Actual:Velocidad_Actual < Recorrido_Actual:Velocidad_Minima_Reglamentaria
        And Tren_Actual:Distancia_A_Proxima_Formacion > 1000 And
        Proximo_Semaforo:Color #= Verde And Tren_Actual:Distancia_A_Proxima_Estacion
        > Proxima_Estacion:Distancia_Minima_Frenado And Tren_Actual:Distancia_A_Proxima_Curva
        > Proxima_Curva:Distancia_Minima_Frenado And Tren_Actual:Transitando_Curva
        #= FALSE;
    },
    SetValue( Tren_Actual:Accion_Sugerida, "Aumente velocidad a "
                                               # Recorrido_Actual:Velocidad_Minima_Permitda ) );

       /*************************************
        ****  RULE: R14.AumentarVelAVelCurva
        *************************************/
MakeRule( R14.AumentarVelAVelCurva, [Tren_Actual|Tren Proximo_Semaforo|Semaforo Proxima_Curva|Curva],
    {
    Tren_Actual:Velocidad_Actual < Proxima_Curva:Velocidad And
        Proximo_Semaforo:Color #= Verde And Tren_Actual:Transitando_Curva
        #= TRUE;
