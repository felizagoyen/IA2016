



/*********************************************************/
/**         ALL FUNCTIONS ARE SAVED BELOW             **/
/*********************************************************/

       /*************************************
        ****  FUNCTION: calcular_distancia_formacion
        *************************************/
MakeFunction( calcular_distancia_formacion, [],
SetValue(Tren_Actual:Distancia_Proxima_Formacion, Recorrido_Actual:Posicion_Siguiente_Tren - Tren_Actual:Posicion) );

       /*************************************
        ****  FUNCTION: calcular_distancia_estacion
        *************************************/
MakeFunction( calcular_distancia_estacion, [],
SetValue(Tren_Actual:Distancia_Proxima_Estacion, Proxima_Estacion:Posicion - Tren_Actual.Posicion) );

       /*************************************
        ****  FUNCTION: calcular_distancia_curva
        *************************************/
MakeFunction( calcular_distancia_curva, [],
SetValue(Tren_Actual:Distancia_Proxima_Curva, Recorrido:Proxima_Curva:Posicion_Inicio - Tren_Actual:Posicion) );

       /*************************************
        ****  FUNCTION: calcular_velocidad_curva
        *************************************/
MakeFunction( calcular_velocidad_curva, [],
SetValue(Recorrido_Actual:Proxima_Curva:Velocidad, 2.2 * Sqrt(Recorrido_Actual:Proxima_Curva:Radio) * Recorrido_Actual:Condicion_Climatica:Factor * Tren_Actual:Factor) );

       /*************************************
        ****  FUNCTION: factor_condicion_climatica
        *************************************/
MakeFunction( factor_condicion_climatica, [],
{
If Recorrido_Actual:Condicion_Climatica:Descripcion #= "despejado"
Then
   SetValue(Recorrido_Actual:Condicion_Climatica:Factor, 1); 

If Recorrido_Actual:Condicion_Climatica:Descripcion #= "nieve"
Then
   SetValue(Recorrido_Actual:Condicion_Climatica:Factor, 0.7); 

If Recorrido_Actual:Condicion_Climatica:Descripcion #= "lluvia"
Then
   SetValue(Recorrido_Actual:Condicion_Climatica:Factor, 0.85); 

If Recorrido_Actual:Condicion_Climatica:Descripcion #= "niebla"
Then
   SetValue(Recorrido_Actual:Condicion_Climatica:Factor, 0.85);

} );

       /*************************************
        ****  FUNCTION: factor_capacidad
        *************************************/
MakeFunction( factor_capacidad, [],
{
If Tren_Actual:Capacidad:Valor >= 75
Then
   SetValue(Tren_Actual:Capacidad:Factor, 1); 

If Tren_Actual:Capacidad:Valor < 75 And Tren_Actual:Capacidad:Valor >= 50
Then
   SetValue(Tren_Actual:Capacidad:Factor, 0.95); 

If Tren_Actual:Capacidad:Valor < 50 And Tren_Actual:Capacidad:Valor >= 25
Then
   SetValue(Tren_Actual:Capacidad:Factor, 0.9); 

If Tren_Actual:Capacidad:Valor < 25
Then
   SetValue(Tren_Actual:Capacidad:Factor, 0.85);

} );

       /*************************************
        ****  FUNCTION: calcular_velocidad_maxima
        *************************************/
MakeFunction( calcular_velocidad_maxima, [],
SetValue(Recorrido_Actual:Velocidad_Maxima_Permitida, Recorrido_Actual:Velocidad_Maxima_Reglamentaria * Recorrido_Actual:Condicion_Climatica:Factor * Tren_Actual:Capacidad:Factor) );

       /*************************************
        ****  FUNCTION: transitando_curva
        *************************************/
MakeFunction( transitando_curva, [],
If Tren_Actual:Posicion < Recorrido_Actual:Proxima_Curva:Posicion_Fin 
    And Tren_Actual:Posicion > Recorrido_Actual:Proxima_Curva:Posicion_Inicio
Then
    SetValue(Tren_Actual:Transitando_Curva, true)
Else
    SetValue(Tren_Actual:Transitando_Curva, false) );

       /*************************************
        ****  FUNCTION: distancia_min_frenado_estacion
        *************************************/
MakeFunction( distancia_min_frenado_estacion, [],
SetValue(Recorrido_Actual:Proxima_Estacion:Distancia_Minima_Frenado, 
    Tren_Actual:Velocidad * 10 / (Recorrido_Actual:Condicion_Climatica:Factor * Tren_Actual:Capacidad:Factor)) );

       /*************************************
        ****  FUNCTION: distancia_min_frenado_curva
        *************************************/
MakeFunction( distancia_min_frenado_curva, [],
SetValue(Recorrido_Actual:Proxima_Estacion:Distancia_Minima_Frenado, 
    (Tren_Actual:Velocidad - Recorrido_Actual:Proxima_Curva:Velocidad) * 10 / (Recorrido_Actual:Condicion_Climatica:Factor * Tren_Actual:Capacidad:Factor)) );

       /*************************************
        ****  FUNCTION: calcular_accion_sugerida
        *************************************/
MakeFunction( calcular_accion_sugerida, [],
{
SetValue(Tren_Actual:Velocidad_Actual, Edit2);
SetValue(Recorrido_Actual:Velocidad_Maxima_Permitida, 120);
PostMessage("asd - " # Tren_Actual:Velocidad_Actual);
} );



